                        <th><p>投保日期</p><p>下期应交日</p></th>
                    </tr>
                </thead>
                <tbody>
                    <% for (let i = 0; i < data.保单信息.length; i++) { %>
                        <tr>
                            <td><a href="/wb?id=<%= data.保单信息[i].id ? data.保单信息[i].id : '-' %>" target="_blank">
                                <p class="mdui-text-truncate"><%= i + 1 %></p>
                            </a></td>
