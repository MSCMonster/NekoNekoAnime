            <div class="mdui-list-item-content">基本信息</div>
            <i class="mdui-collapse-item-arrow mdui-icon material-icons">keyboard_arrow_down</i>
        </div>
        <div class="content-list mdui-collapse-item-body">
            <div class="list-box">
                <ul class="mdui-list">
                    <li class="mdui-list-item mdui-ripple">
                        <i class="mdui-list-item-icon mdui-icon material-icons">account_circle</i>
                        <div class="mdui-list-item-content list-item-title">姓名</div>
                        <div class="mdui-list-item-content list-item-text click-to-copy">
                            <%= data.姓名 %>
                        </div>
                    </li>
                    <li class="mdui-list-item mdui-ripple">
                        <i class="mdui-list-item-icon mdui-icon material-icons">accessibility</i>
